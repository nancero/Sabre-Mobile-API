{
  "name": "@nucleusstudio/sabre-server",
  "version": "0.0.10",
  "description": "Sabre Server App",
  "author": "Nucleus Studio",
  "engines": {
    "node": "12.18.x"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "copy:assets": "cp package.json dist/",
    "build": "cross-env NODE_OPTIONS=--openssl-legacy-provider nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:dev": "nest start --watch",
    "dev": "npx nodemon --exec npx cross-env NODE_ENV=development ts-node -r tsconfig-paths/register src/main.ts",
    "debug": "nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@nestjs/common": "^7.3.2",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.3.2",
    "@nestjs/jwt": "^7.1.0",
    "@nestjs/mongoose": "^7.0.2",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^7.3.2",
    "@nestjs/platform-socket.io": "^7.3.2",
    "@nestjs/schedule": "^0.4.0",
    "@nestjs/serve-static": "^2.1.3",
    "@nestjs/swagger": "^4.5.12",
    "@nestjs/websockets": "^7.3.2",
    "aws-sdk": "^2.713.0",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.2",
    "cache-manager": "^3.3.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.1.3",
    "google-auth-library": "^6.0.4",
    "googleapis": "^52.1.0",
    "got": "^11.5.0",
    "handlebars": "^4.7.6",
    "helmet": "^3.23.3",
    "in-app-purchase": "^1.11.4",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.19",
    "moment": "^2.27.0",
    "mongoose": "^5.9.23",
    "mongoose-paginate-v2": "^1.3.9",
    "nest-logger": "^6.0.0",
    "nodemailer": "^6.4.10",
    "nodemailer-sendgrid": "^1.0.3",
    "onesignal-node": "^3.0.2",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.0",
    "socket.io": "^2.3.0",
    "swagger-ui-express": "^4.1.4",
    "twilio": "^3.48.0",
    "url": "^0.11.0",
    "validator": "^13.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.0.1",
    "@commitlint/config-conventional": "^9.0.1",
    "@nestjs/cli": "^7.4.1",
    "@nestjs/schematics": "^7.0.1",
    "@nestjs/testing": "^7.3.2",
    "@types/express": "^4.17.7",
    "@types/jest": "^26.0.4",
    "@types/mongoose": "^5.7.31",
    "@types/node": "^14.0.22",
    "@types/socket.io": "^2.1.8",
    "@types/supertest": "^2.0.10",
    "cross-env": "^7.0.3",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "lerna": "^3.22.1",
    "lint-staged": "^10.2.11",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "start-server-webpack-plugin": "^2.2.5",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.1",
    "ts-loader": "^8.0.0",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.2",
    "typescript": "^3.9.6",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "tslint -p tsconfig.json -c tslint.json"
    ]
  }
}
